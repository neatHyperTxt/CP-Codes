
#include<iostream>
#define ll long long int

using namespace std;

int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	cout.tie(0);
	ll n;
	cout<<"Enter the Number you want to find the Prime Factors for"<<endl;
	cin>>n;
	cout<<"The Prime Factors are: "<<endl;
	for(ll i=2;i*i<=n;i++)
	{
		while(n%i==0)
		{
			cout<<i<<" ";
			n/=i;
		}
	}
	if(n>1)
	{
		cout<<n<<endl;
	}
	cout<<endl;
	return 0;
}
